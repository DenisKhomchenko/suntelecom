{
  "name": "markup-starter-kit",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "kit:remove-root-rep": "npx rimraf .git",
    "kit:init-project-rep": "git init",
    "kit:init": "npm ci",
    "kit:create": "npm run kit:remove-root-rep && npm run kit:init-project-rep && npm run kit:init",
    "build:prod": "webpack --mode=production --node-env=production",
    "build:dev": "webpack serve --mode=development --node-env=development",
    "fix:js": "npx eslint --fix  \"src/**/*.js\"",
    "fix:css": "npx stylelint --fix \"src/**/*.{css,scss}\"",
    "lint:js": "npx eslint \"src/**/*.js\"",
    "lint:css": "npx stylelint \"src/**/*.{css,scss}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/runtime": "^7.17.2",
    "@squoosh/lib": "^0.4.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "html-beautifier-webpack-plugin": "^1.0.6",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "pug-html-loader": "^1.1.5",
    "remove-files-webpack-plugin": "^1.5.0",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "stylelint": "^8.4.0",
    "stylelint-config-airbnb": "^0.0.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.1",
    "imask": "^6.4.3-alpha.0",
    "normalize.css": "^8.0.1",
    "swiper": "^8.3.1"
  }
}
